		<control type="list" id="200">
			<width>252</width>
			<height>756</height>
			<onup>200</onup>
			<ondown>200</ondown>
			<scrolltime>200</scrolltime>
			<viewtype label="536">list</viewtype>
			<orientation>vertical</orientation>
			<preloaditems>2</preloaditems>
			<itemlayout height="252" width="252">
				<control type="image">
					<left>3</left>
					<top>3</top>
					<width>246</width>
					<height>246</height>
					<texture>views/frame_device_sq.png</texture>
				</control>
				<control type="image">
					<left>18</left>
					<top>18</top>
					<width>216</width>
					<height>216</height>
					<aspectratio>keep</aspectratio><texture fallback="DefaultTVShows.png">$INFO[ListItem.Icon]</texture>
					<bordersize>4</bordersize>
					<bordertexture>views/info_cover_border.png</bordertexture>
				</control>
			</itemlayout>
			<focusedlayout height="252" width="252">
				<control type="image">
					<left>3</left>
					<top>3</top>
					<width>246</width>
					<height>246</height>
					<texture>views/frame_device_sq.png</texture>
				</control>
				<control type="group">
					<visible>!Skin.HasSetting(Hide_Focus_Panel)</visible>
					<control type="image">
						<left>0</left>
						<top>252</top>
						<width>252</width>
						<height>207</height>
						<animation type="Focus">
							<effect type="fade" time="10" delay="250" />
							<effect type="slide" tween="circle" easing="out" start="0,-207" end="0,0" time="250" delay="250" />
						</animation>
						<texture>views/hover_frame_device_capsule.png</texture>
						<visible>Control.HasFocus(200) + [Container(200).Row(0) | Container(200).Row(1)]</visible>
					</control>
					<control type="textbox">
						<left>22</left>
						<top>252</top>
						<width>207</width>
						<height>207</height>
						<font>font21sq</font>
						<align>left</align>
						<aligny>top</aligny>
						<animation type="Focus">
							<effect type="fade" end="100" time="200" delay="300" />
							<effect type="slide" tween="circle" easing="out" start="0,-207" end="0,0" time="250" delay="250" />
						</animation>
						<label>$INFO[ListItem.Label][CR][COLOR lightgrey]$INFO[ListItem.Property(NextTitle)][/COLOR]</label>
						<visible>Control.HasFocus(200) + [Container(200).Row(0) | Container(200).Row(1)]</visible>
					</control>
					<control type="image">
						<left>0</left>
						<top>-207</top>
						<width>252</width>
						<height>207</height>
						<animation type="Focus">
							<effect type="fade" end="100" time="10" delay="250" />
							<effect type="slide" tween="circle" easing="out" start="0,207" end="0,0" time="250" delay="250" />
						</animation>
						<texture flipy="true">views/hover_frame_device_capsule.png</texture>
						<visible>Control.HasFocus(200) + Container(200).Row(2)</visible>
					</control>
					<control type="textbox">
						<left>22</left>
						<top>-207</top>
						<width>207</width>
						<height>207</height>
						<font>font21sq</font>
						<align>left</align>
